---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>So Zetta Slow</title>
  <link href="http://example.com/"/>
  <link type="application/atom+xml" rel="self" href="http://example.com/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://onox.com.br/</id>
  <author>
    <name>Carlos Agarie</name>
    <email>carlos@onox.com.br</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://onox.com.br{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://example.com{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Carlos Agarie</name>
      <uri>http://onox.com.br/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>